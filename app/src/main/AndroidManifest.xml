<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jetpacklearning_kotlin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetPackLearningKotlin"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".weather.WeatherActivity"
            android:exported="false"
            android:label="@string/title_activity_weather"
            android:theme="@style/Theme.JetPackLearningKotlin" />
        <activity
            android:name=".note_app.NoteActivity"
            android:exported="false"
            android:label="@string/title_activity_jet_trivia"
            android:theme="@style/Theme.JetPackLearningKotlin" />
        <activity
            android:name=".movie_and_bill_app.activity.MovieActivity"
            android:exported="false"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.JetPackLearningKotlin" />
        <activity
            android:name=".movie_and_bill_app.activity.MainActivity"
            android:exported="false"
            android:label="@string/title_activity_movie"
            android:theme="@style/Theme.JetPackLearningKotlin" />
        <activity
            android:name=".jet_trivia.JetTriviaActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JetPackLearningKotlin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".movie_and_bill_app.activity.ExcersizeActivity"
            android:exported="false" />
    </application>

</manifest>